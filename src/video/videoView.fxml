<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchor" onMouseDragged="#setOnMouseDragged" onMouseEntered="#setOnMouseEntered" onMouseExited="#setOnMouseExited" onMousePressed="#setOnMousePressed" prefHeight="382.0" prefWidth="568.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="video.VideoController">
   <children>
      <MediaView fx:id="fxMediaView" />
      <AnchorPane fx:id="icons" layoutY="-3.0" prefHeight="389.0" prefWidth="575.0">
         <children>
            <HBox layoutX="6.0" layoutY="336.0" prefHeight="53.0" prefWidth="562.0">
               <children>
                  <Label text="소리">
                     <padding>
                        <Insets left="15.0" top="7.0" />
                     </padding>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>
                  <Slider fx:id="slider" onMouseDragged="#volumnClickProc" showTickMarks="true">
                     <padding>
                        <Insets left="15.0" top="7.0" />
                     </padding>
                  </Slider>
               </children>
               <padding>
                  <Insets left="7.0" right="5.0" top="7.0" />
               </padding>
            </HBox>
            <Label fx:id="btnPlay" alignment="CENTER" contentDisplay="CENTER" layoutX="214.0" layoutY="141.0" onMouseClicked="#playProc" prefHeight="43.0" prefWidth="53.0" text="▶" textFill="WHITE">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Label fx:id="btnPause" alignment="CENTER" contentDisplay="CENTER" layoutX="195.0" layoutY="141.0" onMouseClicked="#pauseProc" prefHeight="22.0" prefWidth="90.0" scaleX="0.5" style="-fx-letter-spacing: 0.5px;" text="❚❚" textFill="WHITE">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Label fx:id="btnStop" alignment="CENTER" contentDisplay="CENTER" layoutX="350.0" layoutY="142.0" onMouseClicked="#stopProc" prefHeight="40.0" prefWidth="54.0" text="■" textFill="WHITE">
               <font>
                  <Font size="48.0" />
               </font>
            </Label>
            <Label fx:id="btnPlus" alignment="CENTER" contentDisplay="CENTER" layoutX="323.0" layoutY="84.0" onMouseClicked="#plusProc" prefHeight="58.0" prefWidth="81.0" style="-fx-font-weight: 600;" text="▷" textFill="WHITE">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Label fx:id="btnMinus" alignment="CENTER" contentDisplay="CENTER" layoutX="137.0" layoutY="113.0" onMouseClicked="#minusProc" prefHeight="58.0" prefWidth="54.0" text="◁" textFill="WHITE">
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            
            
            
            
            <Button fx:id="btnSlow" mnemonicParsing="false" onAction="#slowProc" prefHeight="30.0" prefWidth="70.0" text="x0.5">
               <padding>
                  <Insets left="8.0" right="8.0" top="7.0" />
               </padding>
            </Button>



            <Button fx:id="btnFast" mnemonicParsing="false" onAction="#fastProc" prefHeight="30.0" prefWidth="70.0" text="x2">
               <padding>
                  <Insets left="8.0" right="8.0" top="7.0" />
               </padding>
            </Button>
            
            
            
            
            
            <ProgressIndicator fx:id="progressIndicator" layoutX="530.0" layoutY="450.0" progress="0.0" />
            <Label layoutX="14.0" layoutY="463.0" text="시간" />
      <Slider fx:id="slider1" layoutX="42.0" layoutY="459.0" onMouseClicked="#timeClickProc" onMouseDragged="#timeDragProc" prefHeight="14.0" prefWidth="483.0" showTickMarks="true" />
            <Button fx:id="btnSlow" layoutX="268.0" layoutY="390.0" mnemonicParsing="false" onAction="#slowProc" prefHeight="30.0" prefWidth="70.0" text="x0.5" />
            <Button fx:id="btnFast" layoutX="369.0" layoutY="390.0" mnemonicParsing="false" onAction="#fastProc" prefHeight="30.0" prefWidth="70.0" text="x2" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
