<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="539.0" prefWidth="568.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="video.VideoController">
   <children>
      <MediaView fx:id="fxMediaView" fitHeight="420.0" fitWidth="535.0" layoutX="16.0" layoutY="14.0" />
      <HBox layoutX="3.0" layoutY="490.0" prefHeight="48.0" prefWidth="562.0">
         <children>
            <Button fx:id="btnPlay" mnemonicParsing="false" onAction="#playProc" prefHeight="30.0" prefWidth="70.0" text="재생">
               <padding>
                  <Insets left="8.0" right="8.0" top="7.0" />
               </padding>
            </Button>
            <Button fx:id="btnPause" mnemonicParsing="false" onAction="#pauseProc" prefHeight="30.0" prefWidth="70.0" text="일시정지">
               <padding>
                  <Insets left="8.0" right="8.0" top="7.0" />
               </padding>
            </Button>
            <Button fx:id="btnStop" mnemonicParsing="false" onAction="#stopProc" prefHeight="30.0" prefWidth="70.0" text="정지">
               <padding>
                  <Insets left="8.0" right="8.0" top="7.0" />
               </padding>
            </Button>
            <Button fx:id="btnSlow" mnemonicParsing="false" onAction="#slowProc" prefHeight="30.0" prefWidth="70.0" text="x0.5">
               <padding>
                  <Insets left="8.0" right="8.0" top="7.0" />
               </padding>
            </Button>
            <Button fx:id="btnFast" mnemonicParsing="false" onAction="#fastProc" prefHeight="30.0" prefWidth="70.0" text="x2">
               <padding>
                  <Insets left="8.0" right="8.0" top="7.0" />
               </padding>
            </Button>
            <Button fx:id="btnMinus" mnemonicParsing="false" onAction="#minusProc" prefHeight="30.0" prefWidth="70.0" text="10초전">
               <padding>
                  <Insets left="8.0" right="8.0" top="7.0" />
               </padding>
            </Button>
            <Button fx:id="btnPlus" mnemonicParsing="false" onAction="#plusProc" prefHeight="30.0" prefWidth="70.0" text="10초후">
               <padding>
                  <Insets left="8.0" right="8.0" top="7.0" />
               </padding>
            </Button>
            <Label text="소리">
               <padding>
                  <Insets left="15.0" top="7.0" />
               </padding>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <Slider fx:id="slider" onMouseClicked="#volumnClickProc" onMouseDragged="#volumnDragProc" showTickMarks="true">
               <padding>
                  <Insets left="15.0" top="7.0" />
               </padding>
            </Slider>
         </children>
         <padding>
            <Insets left="7.0" right="5.0" top="7.0" />
         </padding>
      </HBox>
      <ProgressIndicator fx:id="progressIndicator" layoutX="530.0" layoutY="450.0" progress="0.0" />
      <Label layoutX="14.0" layoutY="463.0" text="시간" />
      <Slider fx:id="slider1" layoutX="42.0" layoutY="459.0" onMouseClicked="#timeClickProc" onMouseDragged="#timeDragProc" prefHeight="14.0" prefWidth="483.0" showTickMarks="true" />
   </children>
</AnchorPane>
